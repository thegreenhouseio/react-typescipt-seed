    "redux-mock-store": "^1.2.2",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.5",
    "source-map-loader": "0.1.5",
    "style-loader": "^0.17.0",
    "ts-jest": "19.0.0",
    "ts-loader": "0.8.2",
    "tslint": "^5.2.0",
    "typescript": "2.0.10",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
